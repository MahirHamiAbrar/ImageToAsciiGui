/* COMBO BOX */

QComboBox{
	border: none;
	border-radius: 5px;
	padding-left: 10px;
	color: rgb(220, 220, 220);
	background-color: rgb(52, 52, 54);
	font: 10pt &quot;MS Shell Dlg 2&quot;;
}

QComboBox:hover{
	border: 1px solid;
	border-color: rgb(91, 91, 91);
	border-color: rgba(13, 165, 253, 150);
}

QComboBox::drop-down {
	subcontrol-origin: padding;
	subcontrol-position: right;
	width: 25px;
	height: 15px;
	border: 0;
	border-left: 1px solid rgb(66, 66, 67);
 }

QComboBox QAbstractItemView {
	outline: 0;
	padding: 10px;
	border-radius: 10px;
	color: rgb(222, 222, 222);
	background-color: rgb(59, 59, 62);
	padding: 4px 4px 4px 4px;
	selection-color: rgb(46, 46, 48);
	selection-background-color: rgb(13, 165, 253);
}

QComboBox QAbstractItemView:item{
	border-radius: 4px;
}

QComboBox.smallcombo::drop-down {
	width: 10px;
	height: 10px;
}

QComboBox:disabled{
	color: rgb(80, 80, 80);
	background-color: rgb(50, 50, 52);
}

/*********/

/* TEXT EDIT */
QTextEdit{
	color: rgb(222, 222, 222);
	background: transparent;
	border: 1px solid rgb(43, 43, 45);
	font: 10pt &quot;MS Shell Dlg 2&quot;;
	selection-color: rgb(50, 50, 52);
	selection-background-color: rgb(85, 170, 255);
}

/* STACKED WIDGET */
QStackedWidget{
	background: transparent;
	border: 2px solid;
	border-color: rgb(74, 74, 74);
}
/*********/

/* SCROLL AREA */
QScrollArea{
	background: transparent;
	border: 2px solid;
	border-color: rgb(74, 74, 74);
}
/*********/

/* GROUP BOX */
QGroupBox{
	color: rgb(222, 222, 222);
	background: transparent;
	font: 11pt &quot;MS Shell Dlg 2&quot;;
	border: 2px solid;
	border-color: rgb(74, 74, 74);
	margin-top: 1ex;
}

QGroupBox:hover{
	border: 2px solid rgba(13, 165, 255, 240);
}

QGroupBox::title{
	subcontrol-origin: margin;
	subcontrol-position: top left;
	padding: 0 3px;
}

QGroupBox::indicator{
	width: 13px;
	height: 13px;
}

QGroupBox::indicator:uchecked{
	background-color: rgb(54, 109, 163);
}
/*********/

/* TAB WIDGET */
QTabWidget QStackedWidget{
	border: 0px;
}

QTabWidget::pane {
	border: none;
	border-radius: 10px;
    background: transparent;
	background-color: rgb(46, 46, 48);
	background-color: rgb(50, 50, 50);
	margin-top: 5px;
}

QTabBar{
	border: 0;
	background-color: rgb(55, 55, 58);
	border-radius: 10px;
}

QTabBar::tab {
	color: rgb(222, 222, 222);
	background: transparent;
	border-radius: 5px;
}

QTabBar::tab:selected {
	background-color: rgb(70, 70, 75);
}

QTabBar::tab:!selected {
	background-color: rgb(54, 54, 57);
}

QTabBar::tab:!selected:hover {
	color: rgb(240, 240, 240);
	background-color: rgb(77, 77, 83);
}

QTabBar::tab:selected:hover {
	color: rgb(240, 240, 240);
	background-color: rgb(77, 77, 83);
}

QTabBar::scroller{
	width: 40px;
	border: 0;
	border-radius: 2px;
}

QTabBar QToolButton{
	border: 0;
	border-radius: 2px;
	background: transparent;
}

QTabBar QToolButton:hover{
	background-color: rgb(67, 67, 67);
}

QTabBar QToolButton:enabled {
	color: rgb(13, 165, 253);
}

QTabBar QToolButton:!enabled {
	background: transparent;
}

QTabBar QToolButton:pressed {
	background-color: rgba(67, 67, 67, 200);
}

QTabBar::tab:top, QTabBar::tab:bottom {
    min-width: 20ex;
    margin-right: 1px;
    padding: 5px 10px 5px 10px;
}

QTabBar::tab:top:last, QTabBar::tab:bottom:last,
QTabBar::tab:top:only-one, QTabBar::tab:bottom:only-one {
    margin-right: 0;
}
 
/*********/

/* VERTICAL SCROLLBAR */
QScrollBar:vertical{
	border: none;
	width: 10px;
	margin: 21px 0 21px 0;
	border-radius: 3px;
	background-color: rgb(55, 55, 58);
 }

QScrollBar::handle:vertical{
	width: 5px;
	min-height: 25px;
	margin: 2px;
	border-radius: 3px;
	background-color: rgb(95, 95, 95);
 }

QScrollBar::handle:vertical:hover {
	background-color: rgba(12, 165, 253, 150);
 }

QScrollBar::add-line:vertical {
	border: none;
	height: 15px;
	subcontrol-origin: margin;
	subcontrol-position: bottom;
	margin-bottom: 7px;
	border-bottom-left-radius: 4px;
	border-bottom-right-radius: 4px;
	background-color: rgb(68, 68, 72);
 }

QScrollBar::sub-line:vertical {
	border: none;
	height: 15px;
	subcontrol-origin: margin;
	subcontrol-position: top;
	margin-top: 7px;
	background-color: rgb(68, 68, 72);
	border-top-left-radius: 4px;
	border-top-right-radius: 4px;
 }

QScrollBar::sub-line:vertical:hover, QScrollBar::add-line:vertical:hover{
	background-color: rgba(12, 165, 253, 150);
 }

QScrollBar::sub-line:vertical:pressed, QScrollBar::add-line:vertical:pressed, QScrollBar::handle:vertical:pressed {
	background-color: rgba(12, 165, 253, 250);
 }
 
 QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical, QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
     background: none;
 }

/*********/

/* HORIZONTAL SCROLLBAR */
QScrollBar:horizontal{
    border: none;
    height: 10px;
	background-color: rgb(55, 55, 58);
    margin: 0 21px 0 21px;
    border-radius: 3px;
 }

QScrollBar::handle:horizontal{
    height: 5px;
    min-width: 25px;
    margin: 2px;
    border-radius: 3px;
	background-color: rgb(95, 95, 95);
 }

QScrollBar::handle:horizontal:hover{
	background-color: rgba(12, 165, 253, 150);
 }

QScrollBar::add-line:horizontal {
    border: none;
    width: 15px;
    subcontrol-origin: margin;
    subcontrol-position: right;
    margin-right: 7px;
	background-color: rgb(68, 68, 72);
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
 }

QScrollBar::sub-line:horizontal {
    border: none;
    width: 15px;
    subcontrol-origin: margin;
    subcontrol-position: left;
    margin-left: 7px;
	background-color: rgb(68, 68, 72);
    border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
 }

QScrollBar::sub-line:horizontal:hover, QScrollBar::add-line:horizontal:hover{
	background-color: rgba(12, 165, 253, 150);
 }

QScrollBar::sub-line:horizontal:pressed, QScrollBar::add-line:horizontal:pressed, QScrollBar::handle:horizontal:pressed {
	background-color: rgba(12, 165, 253, 250);
 }
 
 QScrollBar::up-arrow:horizontal, QScrollBar::down-arrow:horizontal, QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
     background: none;
 }

/*********/

/* SLIDERS */

QSlider::groove:horizontal {
	border: none;
	height: 8px;
	border-radius: 4px;
}

QSlider::sub-page:horizontal {
	background: qlineargradient(x1: 0, y1: 0.2, x2: 1, y2: 1, stop: 0 rgb(0, 127, 191), stop: 1 rgb(0, 170, 255));
	border: 0;
	height: 10px;
	border-radius: 4px;
}

QSlider::add-page:horizontal {
	background-color: rgb(46, 46, 48);
	border: 1px solid #777;
	height: 10px;
	border-radius: 4px;
}

QSlider::handle:horizontal {
	background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #eee, stop:1 rgb(50, 50, 52));
	border: 0;
	width: 14px;
	margin-top: -3px;
	margin-bottom: -3px;
	border-radius: 5px;
}

QSlider::handle:horizontal:hover {
	background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #fff, stop:1 rgb(0, 114, 171));
	border: 0;
	border-radius: 5px;
}

QSlider::sub-page:horizontal:disabled {
	background-color: rgb(65, 65, 68);
	border: 1px solid rgb(119, 119, 119);
}

QSlider::add-page:horizontal:disabled {
	background-color: rgb(76, 76, 79);
}

QSlider::handle:horizontal:disabled {
	background-color: rgb(76, 76, 76);
	border: 0;
	border-radius: 5px;
}

