/* LINE EDIT */

QLineEdit{
	border: 2px solid;
	border-color: rgb(74, 74, 74);
	border-radius: 9px;
	color: rgba(222, 222, 222, 222);
	background-color: rgb(50, 50, 52);
	font: 10pt &quot;MS Shell Dlg 2&quot;;
	selection-color: rgb(46, 46, 48);
	selection-background-color: rgba(13, 165, 253, 250);
}

QLineEdit:hover{
	color: rgba(222, 222, 222, 222);
	border-color: rgb(9, 126, 193);
	background-color: rgba(13, 165, 253, 30);
}

QLineEdit:pressed{
	background-color: rgba(0, 170, 255, 100);
}

QLineEdit:focus{
	color: rgb(255, 255, 255);
	border-color: rgba(13, 165, 253, 255);
	background-color: rgba(13, 165, 253, 50);
}

QLineEdit:disabled{
	color: rgb(80, 80, 80);
}

