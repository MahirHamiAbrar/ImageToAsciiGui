/* RADIO BUTTON */
QRadioButton{
	color: rgba(222, 222, 222, 230);
	font: 10pt &quot;MS Shell Dlg 2&quot;;
}

QRadioButton:checked{
	color: rgb(245, 245, 245);
}

QRadioButton:hover{
	color: rgba(13, 165, 253, 230);
}

QRadioButton::indicator {
	width: 13px;
	height: 13px;
	border-radius: 8px;
    border: 3px solid rgb(65, 65, 67);
	background-color: rgb(40, 40, 40);
}

QRadioButton::indicator:hover {
